{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es7",
      "es6",
      "es5"
    ],
    "allowJs": true,
    "jsx": "react",
    "baseUrl": ".",
  }
}
